      <select name="logradouro_prof">
        <option>Avenida</option>
        <option>Rua</option>
        <option>Praça</option>
        <option>Travessa</option>
        <option>Alameda</option>
        <option>Beco</option>
        <option>Passagem</option>
        <option>Estrada</option>
      </select><br>
      Endereço: <input type="text" name="endereco_prof"><br>
      País:
      <select name="pais_prof">
        <option>Brasil</option>
        <option>Argentina</option>
        <option>Outros</option>
      </select><br>
      UF:
      <select name="uf_prof">
        <option>PR</option>
        <option>SP</option>
        <option>RJ</option>
      </select><br>
      CEP: <input type="text" name="cep_prof"><br>
      Cidade: <input type="text" name="cidade_prof"><br>
      Bairro: <input type="text" name="bairro_prof"><br>
      Caixa Postal: <input type="text" name="caixa_postal_prof"><br>
      DDD: <input type="text" name="ddd_prof"><br>
      Telefone: <input type="text" name="tel_prof"><br>
      Ramal: <input type="text" name="ramal_prof"><br>
      Fax: <input type="text" name="fax_prof"><br>
      E-mail: <input type="email" name="email_prof"><br>
      Homepage Institucional: <input type="url" name="homepage_prof">
    </fieldset>

